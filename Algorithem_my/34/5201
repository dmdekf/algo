T = int(input())
for t in range(1,T+1):
    N, M = map(int, input().split())
    w = list(map(int, input().split()))
    td = list(map(int, input().split()))
    s = 0
    while w and td:
        for i in w[:]:
            if i > max(td):
                w.remove(i)
        if w and td:
            s += max(w)
            w.remove(max(w))
            td.remove(max(td))
    if s > 0:
        print('#{} {}'.format(t, s))
    else:
        print('#{} 0'.format(t))